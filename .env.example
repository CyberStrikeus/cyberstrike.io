# Cyberstrike Environment Variables
# Copy this file to .env and fill in your values

# =============================================================================
# AI Provider API Keys (Choose one or more)
# =============================================================================

# Anthropic (Claude)
# ANTHROPIC_API_KEY=your_anthropic_api_key_here

# OpenAI
# OPENAI_API_KEY=your_openai_api_key_here

# Google AI
# GOOGLE_GENERATIVE_AI_API_KEY=your_google_api_key_here

# Cyberstrike Arsenal (managed service)
# CYBERSTRIKE_API_KEY=your_cyberstrike_api_key_here

# GitHub Copilot (OAuth-based, configured via /connect command)

# =============================================================================
# Optional: AWS Bedrock
# =============================================================================
# AWS_ACCESS_KEY_ID=your_aws_access_key
# AWS_SECRET_ACCESS_KEY=your_aws_secret_key
# AWS_REGION=us-east-1

# =============================================================================
# Optional: Azure OpenAI
# =============================================================================
# AZURE_RESOURCE_NAME=your_azure_resource_name

# =============================================================================
# Optional: Cloudflare AI Gateway
# =============================================================================
# CLOUDFLARE_ACCOUNT_ID=your_cloudflare_account_id
# CLOUDFLARE_GATEWAY_ID=your_cloudflare_gateway_id
# CLOUDFLARE_API_TOKEN=your_cloudflare_api_token

# =============================================================================
# Optional: GitLab Duo
# =============================================================================
# GITLAB_INSTANCE_URL=https://gitlab.com
# GITLAB_TOKEN=your_gitlab_token

# =============================================================================
# Development (only for contributors)
# =============================================================================
# CYBERSTRIKE_DEV=1
