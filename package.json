    "zod-to-json-schema": "^3.24.5"
  },
  "dependencies": {
    "@actions/core": "^1.11.1",
    "@actions/github": "^6.0.1",
    "@agentclientprotocol/sdk": "^0.12.0",
    "@ai-sdk/anthropic": "^2.0.57",
    "@ai-sdk/google": "^2.0.52",
