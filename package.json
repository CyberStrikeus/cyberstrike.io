{
  "$schema": "https://json.schemastore.org/package.json",
  "version": "1.0.0",
  "name": "cyberstrike",
  "description": "AI-powered penetration testing and security assessment CLI",
  "type": "module",
  "license": "AGPL-3.0",
  "author": "Cyberstrike Team <team@cyberstrike.io>",
  "homepage": "https://cyberstrike.io",
  "repository": {
    "type": "git",
    "url": "https://github.com/CyberStrikeus/cyberstrike.io"
  },
  "bugs": {
    "url": "https://github.com/CyberStrikeus/cyberstrike.io/issues"
  },
  "keywords": [
    "cybersecurity",
    "penetration-testing",
    "security",
    "ai",
    "cli",
    "pentest",
    "vulnerability-scanner",
    "ethical-hacking",
    "security-tools",
    "red-team"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "os": [
    "darwin",
    "linux",
    "win32"
  ],
  "scripts": {
    "typecheck": "tsc --noEmit",
    "test": "bun test",
    "build": "bun run script/build.ts",
    "dev": "bun run --conditions=browser ./src/index.ts",
    "cyberstrike": "bun run --conditions=browser ./src/index.ts",
    "clean": "rm -rf node_modules dist",
    "prepublishOnly": "bun run build"
  },
  "bin": {
    "cyberstrike": "./bin/cyberstrike"
  },
  "files": [
    "bin/",
    "dist/",
    "src/",
    "LICENSE",
    "README.md"
  ],
  "exports": {
    ".": "./dist/index.js",
    "./*": "./dist/*.js"
  },
  "devDependencies": {
    "@babel/core": "^7.28.4",
    "@octokit/webhooks-types": "^7.6.1",
