{
  "$schema": "https://cyberstrike.io/theme.json",
  "defs": {
    "voidBlack": "#0a0a0f",
    "darkSpace": "#0d0d14",
    "deepSpace": "#12121a",
    "cosmicDark": "#181820",
    "nebulaDark": "#1e1e28",
    "starDust": "#282832",
    "cosmicGray": "#3a3a48",
    "silverMist": "#5a5a6e",
    "moonGlow": "#8888a0",
    "starLight": "#c8c8e0",
    "pureLight": "#f0f0ff",

    "neonCyan": "#00ffff",
    "electricBlue": "#00d4ff",
    "cyberBlue": "#0099ff",
    "deepCyan": "#00b8b8",

    "hotMagenta": "#ff00ff",
    "neonPink": "#ff3399",
    "cyberPink": "#ff66b2",
    "softPink": "#ff99cc",

    "matrixGreen": "#00ff66",
    "neonGreen": "#39ff14",
    "cyberGreen": "#66ff99",
    "limeGlow": "#b3ff66",

    "plasmaPurple": "#9933ff",
    "electricPurple": "#cc66ff",
    "cosmicPurple": "#b366ff",

    "warningYellow": "#ffff00",
    "alertOrange": "#ff9900",
    "fireOrange": "#ff6600",

    "dangerRed": "#ff3333",
    "bloodRed": "#ff0033",
    "alertRed": "#ff4444",

    "lightBg": "#f5f5fa",
    "lightPanel": "#eaeaf0",
    "lightElement": "#e0e0e8",
    "lightBorder": "#c8c8d4",
    "lightText": "#1a1a24",
    "lightMuted": "#6e6e80"
  },
  "theme": {
    "primary": {
      "dark": "neonCyan",
      "light": "cyberBlue"
    },
    "secondary": {
      "dark": "hotMagenta",
      "light": "neonPink"
    },
    "accent": {
      "dark": "plasmaPurple",
      "light": "electricPurple"
    },
    "error": {
      "dark": "dangerRed",
      "light": "bloodRed"
    },
    "warning": {
      "dark": "warningYellow",
      "light": "alertOrange"
    },
    "success": {
      "dark": "matrixGreen",
      "light": "cyberGreen"
    },
    "info": {
      "dark": "electricBlue",
      "light": "cyberBlue"
    },
    "text": {
      "dark": "pureLight",
      "light": "lightText"
    },
    "textMuted": {
      "dark": "moonGlow",
      "light": "lightMuted"
    },
    "background": {
      "dark": "voidBlack",
      "light": "lightBg"
    },
    "backgroundPanel": {
      "dark": "darkSpace",
      "light": "lightPanel"
    },
    "backgroundElement": {
      "dark": "deepSpace",
      "light": "lightElement"
    },
    "backgroundMenu": {
      "dark": "cosmicDark",
      "light": "lightElement"
    },
    "border": {
      "dark": "cosmicGray",
      "light": "lightBorder"
    },
    "borderActive": {
      "dark": "neonCyan",
      "light": "cyberBlue"
    },
    "borderSubtle": {
      "dark": "nebulaDark",
      "light": "lightPanel"
    },
    "diffAdded": {
      "dark": "matrixGreen",
      "light": "cyberGreen"
    },
    "diffRemoved": {
      "dark": "dangerRed",
      "light": "bloodRed"
    },
    "diffContext": {
      "dark": "moonGlow",
      "light": "lightMuted"
    },
    "diffHunkHeader": {
      "dark": "electricBlue",
      "light": "cyberBlue"
    },
    "diffHighlightAdded": {
      "dark": "neonGreen",
      "light": "#2db36e"
    },
    "diffHighlightRemoved": {
      "dark": "alertRed",
      "light": "#d63333"
    },
    "diffAddedBg": {
      "dark": "#0a1f14",
      "light": "#e0f5e8"
    },
    "diffRemovedBg": {
      "dark": "#1f0a0f",
      "light": "#f5e0e4"
    },
    "diffContextBg": {
      "dark": "darkSpace",
      "light": "lightPanel"
    },
    "diffLineNumber": {
      "dark": "starDust",
      "light": "lightBorder"
    },
    "diffAddedLineNumberBg": {
      "dark": "#081810",
      "light": "#d0e8d8"
    },
    "diffRemovedLineNumberBg": {
      "dark": "#18080c",
      "light": "#e8d0d4"
    },
    "markdownText": {
      "dark": "pureLight",
      "light": "lightText"
    },
    "markdownHeading": {
      "dark": "neonCyan",
      "light": "cyberBlue"
    },
    "markdownLink": {
      "dark": "hotMagenta",
      "light": "neonPink"
    },
    "markdownLinkText": {
      "dark": "cyberPink",
      "light": "neonPink"
    },
    "markdownCode": {
      "dark": "matrixGreen",
      "light": "cyberGreen"
    },
    "markdownBlockQuote": {
      "dark": "moonGlow",
      "light": "lightMuted"
    },
    "markdownEmph": {
      "dark": "alertOrange",
      "light": "fireOrange"
    },
    "markdownStrong": {
      "dark": "warningYellow",
      "light": "alertOrange"
    },
    "markdownHorizontalRule": {
      "dark": "cosmicGray",
      "light": "lightBorder"
    },
    "markdownListItem": {
      "dark": "neonCyan",
      "light": "cyberBlue"
    },
    "markdownListEnumeration": {
      "dark": "electricPurple",
      "light": "plasmaPurple"
    },
    "markdownImage": {
      "dark": "hotMagenta",
      "light": "neonPink"
    },
    "markdownImageText": {
      "dark": "cyberPink",
      "light": "neonPink"
    },
    "markdownCodeBlock": {
      "dark": "pureLight",
      "light": "lightText"
    },
    "syntaxComment": {
      "dark": "moonGlow",
      "light": "lightMuted"
    },
    "syntaxKeyword": {
      "dark": "hotMagenta",
      "light": "neonPink"
    },
    "syntaxFunction": {
      "dark": "neonCyan",
      "light": "cyberBlue"
    },
    "syntaxVariable": {
      "dark": "starLight",
      "light": "lightText"
    },
    "syntaxString": {
      "dark": "matrixGreen",
      "light": "cyberGreen"
    },
    "syntaxNumber": {
      "dark": "alertOrange",
      "light": "fireOrange"
    },
    "syntaxType": {
      "dark": "electricPurple",
      "light": "plasmaPurple"
    },
    "syntaxOperator": {
      "dark": "electricBlue",
      "light": "cyberBlue"
    },
    "syntaxPunctuation": {
      "dark": "starLight",
      "light": "lightText"
    },
    "thinkingOpacity": 0.5
  }
}
