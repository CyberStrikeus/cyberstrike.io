{
  "manifest_version": 3,
  "name": "Cyberstrike",
  "version": "1.0.0",
  "description": "Cyberstrike browser automation indicator",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "css": ["styles.css"],
      "run_at": "document_start",
      "all_frames": false
    }
  ],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
