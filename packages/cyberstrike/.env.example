# Cyberstrike Environment Variables
# Copy this file to .env and fill in your values

# AI Provider API Keys (at least one is required)
ANTHROPIC_API_KEY=your_anthropic_api_key_here
OPENAI_API_KEY=your_openai_api_key_here
GOOGLE_GENERATIVE_AI_API_KEY=your_google_ai_key_here
GROQ_API_KEY=your_groq_api_key_here
MISTRAL_API_KEY=your_mistral_api_key_here
XAI_API_KEY=your_xai_api_key_here
DEEPINFRA_API_KEY=your_deepinfra_api_key_here
TOGETHER_API_KEY=your_together_api_key_here
PERPLEXITY_API_KEY=your_perplexity_api_key_here
CEREBRAS_API_KEY=your_cerebras_api_key_here
COHERE_API_KEY=your_cohere_api_key_here

# OpenRouter (alternative to direct provider keys)
OPENROUTER_API_KEY=your_openrouter_api_key_here

# AWS Bedrock (requires AWS credentials)
AWS_ACCESS_KEY_ID=your_aws_access_key_here
AWS_SECRET_ACCESS_KEY=your_aws_secret_key_here
AWS_REGION=us-east-1

# Azure OpenAI
AZURE_OPENAI_API_KEY=your_azure_openai_key_here
AZURE_OPENAI_ENDPOINT=https://your-resource.openai.azure.com

# Google Vertex AI (requires GCP credentials)
GOOGLE_APPLICATION_CREDENTIALS=/path/to/service-account.json
GOOGLE_CLOUD_PROJECT=your_project_id

# Cyberstrike Cloud (optional - for session sharing and team features)
CYBERSTRIKE_API_KEY=your_cyberstrike_api_key_here

# GitHub Integration (optional)
GITHUB_TOKEN=your_github_token_here

# GitLab Integration (optional)
GITLAB_TOKEN_CYBERSTRIKE=your_gitlab_token_here
